{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "coderfee-com",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-06-27",
	"compatibility_flags": [
		"nodejs_compat",
		"global_fetch_strictly_public"
	],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
		"enabled": true
	},
	 "r2_buckets": [
    {
      "binding": "R2_OBSIDIAN",
      "bucket_name": "obsidian",
			"preview_bucket_name": "obsidian"
    }
  ]
}
