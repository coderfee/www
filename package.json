    "dayjs": "^1.11.18",
    "dotenv": "^17.2.1",
    "lefthook": "^1.12.3",
    "unplugin-icons": "^22.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild",
      "lefthook",
      "sharp"
