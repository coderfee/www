    "@commitlint/config-conventional": "^20.3.1",
    "@iconify-json/tabler": "^1.2.26",
    "@iconify/react": "^6.0.2",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "latest",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "lefthook": "^2.0.15",
    "unplugin-icons": "^23.0.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
