    "@astrojs/rss": "4.0.15-beta.1",
    "@astrojs/sitemap": "3.6.1-beta.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "astro": "6.0.0-beta.3",
    "astro-embed": "^0.12.0",
    "astro-pagefind": "^1.8.5",
    "framer-motion": "^12.29.2",
    "ky": "^1.14.3",
    "pagefind": "^1.4.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-autolink-headings": "^7.1.0",
