    "@tailwindcss/vite": "^4.1.12",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "astro": "^5.13.5",
    "astro-embed": "^0.9.0",
    "astro-pagefind": "^1.8.4",
    "framer-motion": "^12.23.12",
    "ky": "^1.9.1",
    "pagefind": "^1.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
