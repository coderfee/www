    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.11",
    "astro-embed": "^0.12.0",
    "astro-pagefind": "^1.8.5",
    "framer-motion": "^12.27.3",
    "ky": "^1.14.3",
    "pagefind": "^1.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
